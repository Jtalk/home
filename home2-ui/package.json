{
  "name": "home2-ui",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev -p ${PORT:-8070}",
    "dev:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run dev",
    "build": "next build",
    "start": "next start -p ${PORT:-80}",
    "start:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:ci:browser": "CYPRESS_CI=true cypress run --headless --record --key ${CYPRESS_DASHBOARD_KEY} --browser",
    "cypress:ci:chrome": "npm run cypress:ci:browser chrome",
    "cypress:ci:firefox": "npm run cypress:ci:browser firefox"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "setupFiles": [
      "./setupTests"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.5.3",
    "@bugsnag/plugin-react": "^7.5.3",
    "@next/bundle-analyzer": "^9.5.5",
    "@zeit/next-source-maps": "0.0.3",
    "dayjs": "^1.9.7",
    "duration-js": "^4.0.0",
    "fuse.js": "^6.4.3",
    "highlight.js": "^10.4.1",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^7.1.0",
    "next": "10.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-flip-move": "^3.0.4",
    "replace-string": "^3.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "sleepjs": "^4.0.2",
    "stacktrace-js": "^2.0.2",
    "storage-available": "^1.1.0",
    "superagent": "^6.1.0",
    "superagent-prefix": "^0.0.2",
    "superagent-suffix": "^0.1.0",
    "swr": "^0.3.9",
    "typechecker": "^7.17.0",
    "uuid": "^8.3.1",
    "webpack": "^4.44.2",
    "webpack-manifest-plugin": "2.2.0",
    "what.js": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "css-loader": "^5.0.1",
    "cypress": "^5.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "react-dev-utils": "^11.0.1",
    "react-test-renderer": "^16.14.0",
    "source-map-explorer": "^2.5.1",
    "superagent-mocker": "^0.5.2",
    "wait-on": "^5.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
