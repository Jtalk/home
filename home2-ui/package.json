{
  "name": "home2-ui",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev -p ${PORT:-8070}",
    "dev:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run dev",
    "build": "next build",
    "start": "next start -p ${PORT:-80}",
    "start:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:ci:browser": "CYPRESS_CI=true cypress run --headless --record --key ${CYPRESS_DASHBOARD_KEY} --browser",
    "cypress:ci:chrome": "npm run cypress:ci:browser chrome",
    "cypress:ci:firefox": "npm run cypress:ci:browser firefox"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "setupFiles": [
      "./setupTests"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.17.0",
    "@bugsnag/plugin-react": "^7.17.0",
    "@jtalk/react-image-loading": "~0.4.0",
    "@next/bundle-analyzer": "^12.2.5",
    "@zeit/next-source-maps": "0.0.3",
    "dayjs": "^1.11.5",
    "duration-js": "^4.0.0",
    "fuse.js": "^6.6.2",
    "highlight.js": "^11.6.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.7",
    "next": "^12.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.4",
    "replace-string": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "sleepjs": "^4.0.3",
    "stacktrace-js": "^2.0.2",
    "storage-available": "^1.1.0",
    "superagent": "^8.0.0",
    "superagent-prefix": "^0.0.2",
    "superagent-suffix": "^0.1.0",
    "swr": "^1.3.0",
    "typechecker": "^7.18.0",
    "uuid": "^8.3.2",
    "webpack": "^5.74.0",
    "what.js": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.6.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-resolve": "^28.1.3",
    "jest-watch-typeahead": "^2.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "react-dev-utils": "^12.0.1",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.2",
    "superagent-mocker": "^0.5.2",
    "wait-on": "^6.0.1"
  }
}
