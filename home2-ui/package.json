{
  "name": "home2-ui",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev -p ${PORT:-8070}",
    "dev:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run dev",
    "build": "next build",
    "start": "next start -p ${PORT:-80}",
    "start:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:ci:browser": "CYPRESS_CI=true cypress run --headless --record --key ${CYPRESS_DASHBOARD_KEY} --browser",
    "cypress:ci:chrome": "npm run cypress:ci:browser chrome",
    "cypress:ci:firefox": "npm run cypress:ci:browser firefox"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "setupFiles": [
      "./setupTests"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.11.0",
    "@bugsnag/plugin-react": "^7.11.0",
    "@jtalk/react-image-loading": "^0.3.0",
    "@next/bundle-analyzer": "^11.1.0",
    "@zeit/next-source-maps": "0.0.3",
    "dayjs": "^1.10.6",
    "duration-js": "^4.0.0",
    "fuse.js": "^6.4.6",
    "highlight.js": "^11.2.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flip-move": "^3.0.4",
    "replace-string": "^3.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "sleepjs": "^4.0.2",
    "stacktrace-js": "^2.0.2",
    "storage-available": "^1.1.0",
    "superagent": "^6.1.0",
    "superagent-prefix": "^0.0.2",
    "superagent-suffix": "^0.1.0",
    "swr": "^0.5.6",
    "typechecker": "^7.18.0",
    "uuid": "^8.3.2",
    "webpack": "^5.51.1",
    "what.js": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "css-loader": "^6.2.0",
    "cypress": "^8.3.0",
    "enzyme": "^3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "jest-resolve": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "mocha": "^9.1.0",
    "prettier": "^2.3.2",
    "react-dev-utils": "^11.0.4",
    "react-test-renderer": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "superagent-mocker": "^0.5.2",
    "wait-on": "^6.0.0"
  }
}
