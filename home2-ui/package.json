{
  "name": "home2-ui",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev -p ${PORT:-8070}",
    "dev:cypress": "SSR_PRELOAD=false DYNAMIC_IMPORT_SSR=false npm run dev",
    "build": "next build",
    "start": "next start -p ${PORT:-80}",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:ci:browser": "CYPRESS_CI=true cypress run --headless --record --key ${CYPRESS_DASHBOARD_KEY} --browser",
    "cypress:ci:chrome": "npm run cypress:ci:browser chrome",
    "cypress:ci:firefox": "npm run cypress:ci:browser firefox"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "setupFiles": [
      "./setupTests"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.0.0",
    "@bugsnag/plugin-react": "^7.0.0",
    "@next/bundle-analyzer": "^9.5.5",
    "@zeit/next-source-maps": "0.0.3",
    "dayjs": "^1.8.24",
    "duration-js": "^4.0.0",
    "fuse.js": "^6.4.3",
    "highlight.js": "^9.18.1",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^6.11.4",
    "next": "10.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flip-move": "^3.0.4",
    "replace-string": "^2.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "sleepjs": "^3.0.2",
    "stacktrace-js": "^2.0.2",
    "storage-available": "^1.1.0",
    "superagent": "^4.1.0",
    "superagent-prefix": "^0.0.2",
    "superagent-suffix": "^0.1.0",
    "swr": "^0.3.8",
    "typechecker": "^4.11.0",
    "uuid": "^3.4.0",
    "webpack": "4.42.0",
    "webpack-manifest-plugin": "2.2.0",
    "what.js": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "css-loader": "3.4.2",
    "cypress": "^5.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mocha": "^8.1.0",
    "prettier": "^2.1.2",
    "react-dev-utils": "^10.2.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.5.0",
    "superagent-mocker": "^0.5.2",
    "wait-on": "^5.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
